# sidney's test rules, if any go here.

# TESTING a few domains that are in our "suspicious" list but are known to have some good senders
describe T_SCC_TLD_FUN From domain in *.fun 
header   T_SCC_TLD_FUN From =~ /<[^ @]+@[^ @>]+\.fun>/

describe T_SCC_TLD_SITE From domain in *.site 
header   T_SCC_TLD_SITE From =~ /<[^ @]+@[^ @>]+\.site>/
